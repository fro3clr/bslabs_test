{
  "plugins": [
        "react"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6":     true,
        "browser": true,
        "node":    true,
        "mocha":   true
    },   
    "extends": [
        "airbnb-base",
        "eslint:recommended", 
        "plugin:react/recommended"
    ],    
   "rules": {
    "react/prop-types": 0
  }
}
